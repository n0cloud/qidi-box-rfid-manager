{
  "expo": {
    "name": "qidi-rfid",
    "slug": "qidi-rfid",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "qidirfid",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NFCReaderUsageDescription": "This app requires NFC to read and write RFID tags for QIDI 3D printer filament management."
      },
      "bundleIdentifier": "app.nocloud.qidirfid"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "app.nocloud.qidirfid",
      "permissions": [
        "android.permission.NFC"
      ],
      "excludeFromChromeOS": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#808080",
          "dark": {
            "backgroundColor": "#808080"
          }
        }
      ],
      "react-native-nfc-manager",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cb3197de-003a-4308-b1e5-01de91a8a528"
      }
    }
  }
}
